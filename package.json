{
	"name": "findkit",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"repository": "github:findkit/wp-findkit",
	"scripts": {
		"test": "tsc && composer phpstan && composer phpcs",
		"tsc": "tsc",
		"phpstan": "composer phpstan",
		"phpcs": "composer phpcs",
		"dev:scripts": "wp-scripts start --output-path=build/scripts src/scripts/*",
		"dev:blocks": "wp-scripts start --output-path=build/blocks",
		"dev": "git valu-hide-changes build && run-p 'dev:*'",
		"build:scripts": "wp-scripts build --output-path=build/scripts src/scripts/*",
		"build:blocks": "wp-scripts build --output-path=build/blocks",
		"build": "git valu-hide-changes build && run-s 'build:*'",
		"build-hide": "git valu-hide-changes build",
		"build-commit": "npm run build && git valu-commit-changes build"
	},
